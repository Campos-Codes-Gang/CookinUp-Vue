{
  "printWidth": 80,
  "tabWidth": 4,
  "useTabs": false,
  "singleQuote": false,
  "semi": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "vueIndentScriptAndStyle": true,
  "quoteProps": "consistent",
  "endOfLine": "auto",
  "proseWrap": "always",
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "parser": "vue",
        "bracketSameLine": false
      }
    }
  ]
}
